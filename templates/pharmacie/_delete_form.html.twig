<form method="post" action="{{ path('pharmacie_delete', {'id': pharmacie.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer la pharmacie?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ pharmacie.id) }}">
    <button class="btn btn-round" onclick="f()">Supprimer</button>
</form>
<script>
    function f (){
        window.alert("Voulez-vous vraiment continuer ?");
    }
</script>